{
    "clientId": "ImpfpassClient",
    "name": "ImpfpassClient",
    "rootUrl": "http://localhost:4200",
    "adminUrl": "http://localhost:4200",
    "baseUrl": "/login",
    "surrogateAuthRequired": false,
    "enabled": true,
    "clientAuthenticatorType": "client-secret",
    "redirectUris": [
        "http://localhost:4006"
    ],
    "webOrigins": [],
    "notBefore": 0,
    "bearerOnly": false,
    "consentRequired": false,
